{
  "name": "lerna-boilerplate",
  "version": "0.0.0",
  "description": "boilerplate for monorepo lerna",
  "main": "index.js",
  "scripts": {
    "version-bump": "lerna version --conventional-commits --tag-version-prefix='' --changelog-preset eslint"
